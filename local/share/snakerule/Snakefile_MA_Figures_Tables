rule supplementary_table_4_150X_williams:
    input: williams=REF_DATASET+'/williams_bin_0.12_0.24.r2.tsv', gen_len=REF_DATASET+'/len_cnall.txt', b_d=B_D, MR=REF_DATASET+"/MR_edu_SNV"
