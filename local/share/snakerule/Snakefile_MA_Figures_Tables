rule source_data_assembly:
    input: F2A="SD/SD_Fig2A.tsv", F5B="SD/SD_Fig5B.tsv", F5C="SD/SD_Fig5C.tsv", SF2="SD/SD_SFig2.tsv", SF3B="SD/SD_SFig3B.tsv", SF4A="SD/SD_SFig4A.tsv", SF4B="SD/SD_SFig4B.tsv",
            SF6A="SD/SD_SFig6A.tsv", SF6B="SD/SD_SFig6B.tsv", SF12A="SD/SD_SFig12A.tsv"
    output: "SD/data_file_S1.xlsx"
    shell:
        """
         tsv_to_xls -i {input.F2A},{input.F5B},{input.F5C},{input.SF2},{input.SF3B},{input.SF4A},{input.SF4B},{input.SF6A},{input.SF6B},{input.SF12A}  \\
         -s Fig.2A,Fig.5B,Fig.5C,SupFig2,SupFig.3B,SupFig.4A,SupFig.4B,SupFig.6A,SupFig.6B,SupFig.12A -o {output}
        """